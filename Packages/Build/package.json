    "lazysizes": "~5.2.2",
    "lg-thumbnail.js": "~1.2.0",
    "lg-video.js": "~1.3.0",
    "lightgallery.js": "~1.4.0",
    "lodash-es": "~4.17.15",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "normalize-scss": "~7.0.1",
    "popper.js": "^1.14.7",
    "progressbar.js": "^1.1.0",
    "quicklink": "~2.0.0",
    "swiper": "^8.4.5",
    "tiny-slider": "~2.9.3",
    "tsparticles": "^2.1.3",
    "tsparticles-engine": "^2.1.3",
    "waypoints": "~4.0.1"
  },
  "scripts": {
    "// -- BUNDLED TASKS": "echo \"bundled tasks which run all at once. INFO: 'watch:all' does run the tasks first normal and then incremental\"",
    "watch:all": "check-node-version --package && webpack --watch",
    "build:all": "check-node-version --package && webpack",
    "// -- WATCHER": "echo \"watcher tasks for the .js and .scss files\"",
    "watch:js": "check-node-version --package && webpack --config-name=scripts --watch",
    "watch:scss": "check-node-version --package && webpack --config-name=styles --watch",
    "// -- COMPILING": "echo \"compiler tasks for the .js and .scss files\"",
    "compile:js": "check-node-version --package && webpack --config-name=scripts",
    "compile:scss": "check-node-version --package && webpack --config-name=styles"
  }
}
