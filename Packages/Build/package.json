  "engines": {
    "node": "^12"
  },
  "devDependencies": {
    "@babel/core": "~7.12.9",
    "@babel/preset-env": "~7.12.7",
    "autoprefixer": "~9.8.6",
    "babel-loader": "~8.2.0",
    "check-node-version": "~4.0.3",
    "core-js": "~3.8.0",
    "css-loader": "~4.3.0",
    "cssnano": "~4.1.10",
    "dotenv": "~8.2.0",
    "eslint": "~7.14.0",
