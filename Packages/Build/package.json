    "eslint-plugin-import": "~2.22.1",
    "glob": "~7.1.6",
    "handlebars-loader": "~1.7.1",
    "mini-css-extract-plugin": "~0.12.0",
    "postcss-loader": "~4.1.0",
    "sass": "~1.29.0",
    "sass-loader": "~10.1.0",
    "upath": "~1.2.0",
    "webpack": "~4.44.2",
    "webpack-bundle-analyzer": "~3.9.0",
    "webpack-cli": "~3.3.12",
    "webpack-fix-style-only-entries": "~0.6.0",
    "webpack-notifier": "~1.11.0",
    "webpackbar": "~4.0.0"
  },
  "dependencies": {
    "animated-scroll-to": "~2.0.12",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.3.1",
    "bootstrap-datepicker": "^1.9.0",
    "cash-dom": "~8.1.0",
    "cookie-parser": "^1.4.6",
    "countup.js": "^2.0.7",
